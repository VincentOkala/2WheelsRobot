#include <stdbool.h>
#include <stdint.h>

#include <slib/system/System.h>
#include <slib/console/Console.h>

#include <modules/GY521/GY521.h>

/**
 * main.c
 */

int main(void)
{
    System::init();
    GY521 gy521 = GY521(I2C_1, GYRO_SCALE_250, ACCEL_SCALE_2G);
    while(1){
        Console::printf("hi %d \r\n", (int)(1000*gy521.getRoll()));
        System::delayMs(500);
    }
}
