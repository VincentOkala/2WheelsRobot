#include <stdbool.h>
#include <stdint.h>

#include <driverlib/sysctl.h>
#include <slib/TIME/TIME.h>
#include <modules/GY521/GY521.h>
#include <slib/CONSOLE/CONSOLE.h>
#include <slib/STATESYSTEM/StateSystem.h>
#include <modules/DRV8412/DRV8412.h>

/**
 * main.c
 */

int main(void)
{
    //Set the system clock to 80Mhz
     SysCtlClockSet(SYSCTL_SYSDIV_2_5|SYSCTL_USE_PLL|SYSCTL_OSC_MAIN|SYSCTL_XTAL_16MHZ);
     DRV8412 driver = DRV8412(PWM1, PWM_GEN_0, PORTD, GPIO_PIN_0, PWM_OUT_0_BIT, GPIO_PD0_M1PWM0, GPIO_PIN_1, PWM_OUT_1_BIT, GPIO_PD1_M1PWM1, PORTD, GPIO_PIN_2, GPIO_PIN_3);
     driver.setVelocityLeft(-20);
     driver.setVelocityRight(-20);
    while(1){

    }
}
