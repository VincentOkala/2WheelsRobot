#include <stdbool.h>
#include <stdint.h>

#include <driverlib/sysctl.h>
#include <slib/TIME/TIME.h>
#include <modules/GY521/GY521.h>
#include <slib/CONSOLE/CONSOLE.h>
#include <slib/STATESYSTEM/StateSystem.h>
#include <peripheral/PWM/PWM.h>
#include <peripheral/GPIO/GPIO.h>

/**
 * main.c
 */

int main(void)
{
    //Set the system clock to 80Mhz
     SysCtlClockSet(SYSCTL_SYSDIV_2_5|SYSCTL_USE_PLL|SYSCTL_OSC_MAIN|SYSCTL_XTAL_16MHZ);

      GPIO portF = GPIO(PORTF);
      portF.mode(GPIO_PIN_1, MODE_OUTPUT);
      portF.mode(GPIO_PIN_2, MODE_OUTPUT);
      portF.mode(GPIO_PIN_3, MODE_OUTPUT);

    while(1){
          portF.write(GPIO_PIN_1, VALUE_ON);
          portF.write(GPIO_PIN_2, VALUE_ON);
          portF.write(GPIO_PIN_3, VALUE_ON);
	      SysCtlDelay(13333333);
		  portF.write(GPIO_PIN_1, VALUE_OFF);      
  		  portF.write(GPIO_PIN_2, VALUE_OFF);
          portF.write(GPIO_PIN_3, VALUE_OFF);
          SysCtlDelay(13333333);
    }
}
